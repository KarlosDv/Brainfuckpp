#includes libs/system.js
#includes libs/controlflow.js

var main: proc
var loop: byte
var out: char

exec {
    |Initialize Values|
    call(main)
    setByte(loop[0] 10)
    setString(out "a")
}

#section preproc

proc main {
    |Prints out first 10 characters from "a"|

    dwhile(loop)
        print(out)
        addByte(out[0] 1)
    end;
    
    uncall(ProcLoop)
}