#includes libs/system.js
#includes libs/controlflow.js

var main: proc
var yellOut: proc
var loop: byte
var out: char[6]

exec {
    |Initialize Values|
    call(main)
    setByte(loop[0] 10)
    read(out)
}

#section preproc

proc main {
    |Calls yellOut process 10 variable loop times!|

    dwhile( loop[0] )
        call(yellOut)
    end;

    uncall(ProcLoop)
}

proc yellOut {
    |Prints variable out|
    print(out)
}